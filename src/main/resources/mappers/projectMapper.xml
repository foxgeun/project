<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.project.dao.ProjectDao">
<select id="getList" parameterType="project">
 	select * from test order by num asc
</select>

<insert id="insertData" parameterType="Project">
  insert into test values(#{num},#{name},#{day})
</insert>


</mapper>